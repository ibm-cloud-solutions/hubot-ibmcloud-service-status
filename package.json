{
  "name": "hubot-ibmcloud-service-status",
  "version": "0.0.32",
  "description": "Hubot script for obtaining IBM Cloud status for regions and specific service plans.",
  "respository": {
    "url": "https://github.com/ibm-cloud-solutions/hubot-ibmcloud-service-status",
    "type": "git"
  },
  "scripts": {
    "lint": "eslint src/**/*.js test/*.js index.js",
    "start": "source config/env && hubot -a shell",
    "slack": "source config/env && hubot -a slack",
    "facebook": "source config/env && hubot -a fb",
    "postinstall": "initDb src/nlc/NLC.json",
    "test": "mocha --require coffee-script/register --compilers coffee:coffee-script test",
    "coverage": "istanbul cover -x '**/tools/**' --include-all-sources --report lcov --report cobertura --report text --dir ./gh-pages/coverage _mocha -- test",
    "release": " node tools/publish"
  },
  "author": "ibm-cloud-solutions",
  "license": "Apache-2.0",
  "dependencies": {
    "bluebird": "^3.4.0",
    "hubot-ibmcloud-cognitive-lib": ">=0.0.26",
    "hubot-ibmcloud-activity-emitter": ">=0.0.3",
    "hubot-ibmcloud-utils": ">=0.0.8",
    "superagent": "^1.8.3",
    "whacko": "^0.19.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "eslint": "^2.9.0",
    "eslint-config-strongloop": "^2.0.1",
    "hubot-help": "^0.2.0",
    "hubot-ibmcloud-auth": ">=0.0.14",
    "hubot-ibmcloud-nlc": ">=0.0.25",
    "hubot-test-helper": "^1.4.4",
    "hubot-fb": "^3.0.0",
    "hubot-shell": "^1.0.2",
    "hubot-slack": "^3.4.2",
    "istanbul": "0.4.0",
    "istanbul-cobertura-badger": "1.2.1",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "shelljs": "^0.7.0"
  },
  "peerDependencies": {
    "hubot": "^2.19.0",
    "hubot-ibmcloud-formatter": ">=0.0.29",
    "i18n-2": "^0.6.3"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "main": "index.js",
  "files": [
    "src",
    "lib",
    "index.js"
  ],
  "keywords": [
    "hubot",
    "hubot-scripts",
    "watson",
    "ibm",
    "translation",
    "slack"
  ]
}
